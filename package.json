{
  "name": "wino",
  "version": "1.0.0",
  "description": "Wine Wise",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "concat-stream": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^7.4.3",
    "jspdf": "^1.5.3",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.11",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "semantic-ui-css": "^2.4.1",
    "vm": "^0.1.0"
  }
}
