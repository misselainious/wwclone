{
  "name": "wino",
  "version": "1.0.0",
  "description": "Wine Wise",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "concat-stream": "^2.0.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-jwt": "^7.7.7",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.5.1",
    "mongodb": "^3.7.3",
    "mongoose": "^5.13.13",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.4.1",
    "semantic-ui-css": "^2.4.1",
    "vm": "^0.1.0"
  }
}
